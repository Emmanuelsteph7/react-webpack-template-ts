{
  "presets": [
    "@babel/preset-env",
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ],
    "@babel/preset-typescript"
  ],
    "plugins": [
      [
        // this lets us use the async-await feature in the react app
        "@babel/plugin-transform-runtime",
        {
          "regenerator": true
        }
      ]
    ]
}
